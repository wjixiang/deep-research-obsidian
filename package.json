{
  "name": "open-deep-research-obsidian",
  "version": "1.0.0",
  "description": "This is a ai research plugin for Obsidian (https://obsidian.md)",
  "main": "main.js",
  "scripts": {
    "dev": "node esbuild.config.mjs",
    "build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
    "version": "node version-bump.mjs && git add manifest.json versions.json",
    "format": "prettier --write \"src/**/*.{ts,tsx}\"",
    "tsx": "tsx --env-file=.env.local",
    "start": "tsx --env-file=.env.local src/run.ts",
    "docker": "tsx src/run.ts",
    "test": "jest",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@chromatic-com/storybook": "^3.2.4",
    "@ianvs/prettier-plugin-sort-imports": "^4.4.1",
    "@storybook/addon-essentials": "^8.5.5",
    "@storybook/addon-interactions": "^8.5.5",
    "@storybook/addon-onboarding": "^8.5.5",
    "@storybook/blocks": "^8.5.5",
    "@storybook/react": "^8.5.5",
    "@storybook/react-vite": "^8.5.5",
    "@storybook/test": "^8.5.5",
    "@types/jest": "^29.5.14",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^22.13.0",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@typescript-eslint/eslint-plugin": "5.29.0",
    "@typescript-eslint/parser": "5.29.0",
    "builtin-modules": "3.3.0",
    "esbuild": "0.17.3",
    "jest": "^29.7.0",
    "obsidian": "latest",
    "prettier": "^3.4.2",
    "ts-jest": "^29.2.5",
    "tslib": "2.4.0",
    "tsx": "^4.19.2",
    "typescript": "^5.7.3"
  },
  "dependencies": {
    "@ai-sdk/openai": "^1.1.9",
    "@mendable/firecrawl-js": "^1.16.0",
    "@xyflow/react": "^12.4.3",
    "ai": "^4.1.17",
    "axios": "^1.7.7",
    "dotenv": "^16.4.7",
    "esbuild-plugin-copy": "^2.1.1",
    "js-tiktoken": "^1.0.17",
    "lodash-es": "^4.17.21",
    "p-limit": "^6.2.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "reactflow": "^11.11.4",
    "storybook": "^8.5.5",
    "styled-components": "^6.1.15",
    "zod": "^3.24.1",
    "zustand": "^5.0.3"
  },
  "engines": {
    "node": "22.x"
  }
}
