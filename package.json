{
	"name": "open-deep-research-obsidian",
	"version": "1.0.0",
	"description": "This is a ai research plugin for Obsidian (https://obsidian.md)",
	"main": "main.js",
	"scripts": {
		"dev": "node esbuild.config.mjs",
		"build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
		"version": "node version-bump.mjs && git add manifest.json versions.json",
		"format": "prettier --write \"src/**/*.{ts,tsx}\"",
		"tsx": "tsx --env-file=.env.local",
		"start": "tsx --env-file=.env.local src/run.ts",
		"docker": "tsx src/run.ts",
		"test": "jest"
	},
	"keywords": [],
	"author": "",
	"license": "MIT",
	"devDependencies": {
		"@types/react": "^18.3.12",
		"@types/react-dom": "^18.3.1",
		"@typescript-eslint/eslint-plugin": "5.29.0",
		"@typescript-eslint/parser": "5.29.0",
		"builtin-modules": "3.3.0",
		"esbuild": "0.17.3",
		"obsidian": "latest",
		"tslib": "2.4.0",
		"@ianvs/prettier-plugin-sort-imports": "^4.4.1",
		"@types/jest": "^29.5.14",
		"@types/lodash-es": "^4.17.12",
		"@types/node": "^22.13.0",
		"jest": "^29.7.0",
		"prettier": "^3.4.2",
		"ts-jest": "^29.2.5",
		"tsx": "^4.19.2",
		"typescript": "^5.7.3"
	},
	"dependencies": {
		"axios": "^1.7.7",
		"esbuild-plugin-copy": "^2.1.1",
		"react": "^18.3.1",
		"react-dom": "^18.3.1",
		"@ai-sdk/openai": "^1.1.9",
		"@mendable/firecrawl-js": "^1.16.0",
		"ai": "^4.1.17",
		"dotenv": "^16.4.7",
		"js-tiktoken": "^1.0.17",
		"lodash-es": "^4.17.21",
		"p-limit": "^6.2.0",
		"zod": "^3.24.1"
	},
	"engines": {
		"node": "22.x"
	}
}
